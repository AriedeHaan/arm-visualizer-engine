{
  "extends": "tslint:recommended",
  "rules": {
    "interface-name": [true, "never-prefix"],
    "array-type": [true, "array"],
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
    "trailing-comma": [false],
    "object-literal-sort-keys": false,
    "member-access": false,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ]
  }
}
